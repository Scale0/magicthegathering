{% extends 'base.html.twig' %}

{% block title %}FieldType{% endblock %}

{% block body %}
    <h1>FieldType</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ field_type.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ field_type.name }}</td>
            </tr>
            <tr>
                <th>Symbol</th>
                <td>{{ field_type.symbol }}</td>
            </tr>
            <tr>
                <th>Blocking</th>
                <td>{{ field_type.blocking ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('field_type_index') }}">back to list</a>

    <a href="{{ path('field_type_edit', {'id': field_type.id}) }}">edit</a>

    {{ include('field_type/_delete_form.html.twig') }}
{% endblock %}
